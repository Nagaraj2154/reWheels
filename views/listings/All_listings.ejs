<% layout('layouts/boilerPlate') -%>

<div class="container mt-4">
 

  <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 g-4">
    <% for(let data of car) { %>
      <a class="ancher text-decoration-none text-dark" href="/listing/<%= data._id %>">
        <div class="card shadow-sm h-100">
          <img src="<%= data.image.url %>" class="card-img-top image" alt="<%= data.name %>">
          <div class="card-img-overlay"></div>
          <div class="card-body">
            <p class="card-text fw-medium mb-1"><%= data.name %></p>
            <h5 class="fw-light h5">
              <i class="fa-solid fa-indian-rupee-sign"></i>
              <%= Math.round(data.carDetails.predicted_price* 100000).toLocaleString("en-IN") %>
            </h5>
          </div>
        </div>
      </a>
    <% } %>
  </div>
</div>
